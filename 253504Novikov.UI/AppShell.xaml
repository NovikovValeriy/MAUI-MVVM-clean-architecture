<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="_253504Novikov.UI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:_253504Novikov.UI"
    xmlns:pages="clr-namespace:_253504Novikov.UI.Pages"
    Title="_253504Novikov.UI">

    <ShellContent
        Icon="home.png"
        Shell.BackgroundColor="BlueViolet"
        Shell.TitleColor="White"
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Icon="menu_burger.png"
        Shell.BackgroundColor="BlueViolet"
        Shell.TitleColor="White"
        Title="Garages list"
        ContentTemplate="{DataTemplate pages:Garages}"
        Route="Garages" />

</Shell>
